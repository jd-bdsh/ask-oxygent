{
  "name": "@bdsh/ask-oxygent",
  "version": "1.0.4",
  "description": "A starter for creating a TypeScript package.",
  "keywords": [
    "agent",
    "oxygent",
    "toolkit"
  ],
  "homepage": "https://github.com/jd-bdsh/ask-oxygent#readme",
  "bugs": {
    "url": "https://github.com/jd-bdsh/ask-oxygent/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jd-bdsh/ask-oxygent.git"
  },
  "license": "MIT",
  "author": "YONGQI HE <heyongqi10@jd.com>",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "format": "prettier",
    "format:check": "prettier --write .",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "bumpp": "10.3.2",
    "oxlint": "1.38.0",
    "prettier": "3.7.4",
    "tsdown": "0.18.2",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
